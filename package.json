{
  "name": "mern-stack-pdf-generator",
  "version": "1.0.0",
  "description": "MERN Stack PDF Generator with Puppeteer backend and React frontend",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start": "cd backend && npm start"
  },
  "keywords": ["mern", "pdf", "generator", "puppeteer", "react", "typescript"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
